<Window x:Class="DisposePlayer.FlyleafWPFControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DisposePlayer"

        xmlns:flwpf="clr-namespace:FlyleafLib.Controls.WPF;assembly=FlyleafLib.Controls.WPF"

        mc:Ignorable="d"
        Title="FlyleafWPFControl" Height="450" Width="800">
    <Grid>
        <flwpf:FlyleafME x:Name="flyleafControl" Player="{Binding Player}"/>
    </Grid>
</Window>
